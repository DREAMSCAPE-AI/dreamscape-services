# DREAMSCAPE AI Service - Environment Variables
# Copy this file to .env and fill in the values

# Database connection
DATABASE_URL=postgresql://dreamscape:password@localhost:5432/dreamscape

# ML ETL Configuration
DATASET_VERSION=1.0
DATA_WINDOW_DAYS=90
NEGATIVE_SAMPLE_RATIO=2.0
TEST_SIZE=0.2
RANDOM_SEED=42

# Logging
LOG_LEVEL=INFO
LOG_FILE=logs/etl.log

# Data directories
DATA_DIR=./data
RAW_DATA_DIR=./data/raw
PROCESSED_DATA_DIR=./data/processed
DATASETS_DIR=./data/datasets
